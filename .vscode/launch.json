{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'rs-fxr'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=rs-fxr"
                ],
                "filter": {
                    "name": "rs-fxr",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'beep'",
            "cargo": {
                "args": [
                    "build",
                    "--example=beep",
                    "--package=rs-fxr"
                ],
                "filter": {
                    "name": "beep",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'json'",
            "cargo": {
                "args": [
                    "build",
                    "--features=json",
                    "--example=json",
                    "--package=rs-fxr"
                ],
                "filter": {
                    "name": "json",
                    "kind": "example"
                }
            },
            "args": [
                "target/Default 1.jfxr"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug example 'wav'",
            "cargo": {
                "args": [
                    "build",
                    "--example=wav",
                    "--package=rs-fxr"
                ],
                "filter": {
                    "name": "wav",
                    "kind": "example"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}